{
  "name": "backstage-manager",
  "version": "1.0.0",
  "description": "> A starter kit for Element UI generated",
  "scripts": {
    "mock": "./node_modules/.bin/ejs-mock start",
    "build": "rimraf dist && webpack --config build/webpack.config.js",
    "start": "cross-env NODE_ENV=development node server",
    "build-test": "rimraf dist && yarn run build:test",
    "build-pro": "rimraf static && yarn run build:pro",
    "build:test": "cross-env NODE_ENV=development webpack --config build/webpack.config.js",
    "build:pro": "cross-env NODE_ENV=production webpack --config build/webpack.config.js",
    "analyzer": "rimraf dist && cross-env NODE_ENV=analyzer webpack --config build/webpack.config.js",
    "stats": "webpack --profile --json > stats.json --config build/webpack.config.js",
    "build-node": "node build/move_node.js"
  },
  "engines": {
    "node": ">=6.11.5"
  },
  "repository": "git@115.29.52.102:liuwh/order-mannager.git",
  "author": "heung",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-3": "^6.22.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "ejs-mock": "0.0.13",
    "element-ui": "^2.3.8",
    "eslint": "^3.18.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-vue": "^2.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.16.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.5.2",
    "vue-loader": "^15.0.11",
    "webpack": "^4.8.3",
    "webpack-bundle-analyzer": "^2.8.2"
  },
  "main": "src/main.js",
  "dependencies": {
    "axios": "^0.16.0",
